<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" default-autowire="byName"
	default-lazy-init="true"
	xsi:schemaLocation="http://www.springframework.org/schema/beans     
	    http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

	<bean class="com.nbw.testTech.dao.TestTechDAO" id="testTechdao" parent="baseDao" />
	
	<bean id="testTechmanager" parent="baseTransactionProxy">
		<property name="target">
			<bean class="com.nbw.testTech.service.TestTechManager" />
		</property>
	</bean>
	
	<bean class="com.nbw.testTech.web.action.TestTechController" name="/testTech.action">
		<property name="methodNameResolver">
			<ref bean="paramResolver" />
		</property>
		<property name="sessionForm">
			<value>false</value>
		</property>
		<property name="commandClass">
			<value>com.nbw.testTech.domain.TestTech</value>
		</property>
	</bean>
	
</beans>